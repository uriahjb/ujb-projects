%% Define the PUMA260's DH parameters:
classdef Puma260
  properties
  L
  plot
  
  methods
      function self = Puma260()
    clear self.L
    self.L = [];
    self.L{1} = [ pi/2   0.0	   0	13.0 ]; 
    self.L{2} = [ 0.0    8.0	   0	-2.5 ];
    self.L{3} = [ -pi/2  0.0    0	-2.5 ];
    seL{4} = [ pi/2   0.0	   0     8.0 ]; 
    L{5} = [ -pi/2  0.0    0     0.0 ];
    L{6} = [ 0.0    0.0    0    7.25 ];
    
 %% Define a few poses
    qz = [0, 0, 0, 0, -pi/2, pi]; % "zero" angled pose.
    